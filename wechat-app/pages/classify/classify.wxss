/* pages/classify/classify.wxss */
.banner-view{
    width:100%;
    height:412rpx;
}

.banner-img{
    width:100%;
    height:100%;
}

.view{
    width:750rpx;
    padding:0 0 0 30rpx;
    box-sizing:border-box;
}

.type-view{
    width:720rpx;
    height:88rpx;
    padding:20rpx 30rpx 20rpx 0;
    box-sizing:border-box;
    display:flex;
    justify-content: space-between;
    align-items:center;
    border-bottom:1rpx solid #ebedf0;
    color:#323233;
}

.icon-img{
    width:32rpx;
    height:48rpx;
}